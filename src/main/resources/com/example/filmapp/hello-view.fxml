<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER_LEFT" spacing="5.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.filmapp.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>
  <HBox alignment="BASELINE_LEFT">
    <Label fx:id="seriesTitle" wrapText="true" maxWidth="300">
      <font>
        <Font name="System Bold" size="24.0" />
      </font>
    </Label>
    <Label fx:id="airDate" wrapText="true" maxWidth="300"/>
  </HBox>
  <Label fx:id="welcomeText" wrapText="true" maxWidth="300"/>
</VBox>
